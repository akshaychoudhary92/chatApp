{
    "extends": "@chrisaguilar/tslint-config",
    "rules": {
        "arrow-parens": [true, "always"],
        "completed-docs": false,
        "jsx-curly-spacing": false,
        "linebreak-style": false,
        "member-access": false,
        "no-import-side-effect": false,
        "strict-type-predicates": false,
        "typedef": false
    },
    "jsRules": {
        "arrow-parens": [true, "always"],
        "completed-docs": false,
        "jsx-curly-spacing": false,
        "linebreak-style": false,
        "no-import-side-effect": false
    }
}
